{
  "manifest_version": 2,
  "name": "Cpex \"Legends\"",
  "short_name": "Cpex",
  "version": "3.1",
  "description": "CoursePowerを使いやすくするChrome拡張です",
  "icons": {
      "16": "16.png",
      "48": "icon.png"
  },

  "content_scripts": [
      {
        "matches": ["*://*/*"],
        "css": ["style.css"],
        "js": ["jquery.min.js", "cs.js"]
      }
  ],

  "options_ui": {
    "page": "opt.html",
    "chrome_style": false    
  },

  "permissions": [
      "activeTab",
      "storage"
  ]
}
